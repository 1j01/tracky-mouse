{
	"private": true,
	"name": "tracky-mouse-monorepo",
	"description": "This package contains scripts shared between packages.",
	"version": "1.0.0",
	"CAN'T USE NPM WORKSPACES, see: https://github.com/1j01/tracky-mouse/issues/53": [
		"core",
		"desktop-app",
		"website"
	],
	"scripts": {
		"website": "cd website && npm run start",
		"website:npm": "cd website && npm",
		"desktop-app": "cd desktop-app && npm run start",
		"desktop-app:npm": "cd desktop-app && npm",
		"core:npm": "cd core && npm",
		"lint": "cspell-cli lint . && eslint"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@types/eslint": "^8.56.9",
		"cspell-cli": "^8.7.0",
		"eslint": "^9.0.0"
	}
}
